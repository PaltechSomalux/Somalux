  FiMenu,\r\n  FiBookmark,\r\n  FiSearch,\r\n  FiColumns,\r\n  FiPlay,\r\n  FiEdit3,\r\n  FiLayout,\r\n} from 'react-icons/fi';\r\nimport './EdgeLikeReader.css';\r\n\r\npdfjs.GlobalWorkerOptions.workerSrc = new URL(\r\n  'pdfjs-dist/build/pdf.worker.min.mjs',\r\n  import.meta.url\r\n).toString();\r\n\r\nconst EdgeLikeReader = ({ src, title, author, onClose, userId, bookId, pages = 0 }) => {\r\n  const [numPages, setNumPages] = useState(null);\r\n  const [pageNumber, setPageNumber] = useState(1);\r\n  const [scale, setScale] = useState(1.0);\r\n  const [rotation, setRotation] = useState(0);\r\n  const [isLoading, setIsLoading] = useState(true);\r\n  const [showSidebar, setShowSidebar] = useState(true);\r\n  const [sidebarTab, setSidebarTab] = useState('thumbnails'); // 'thumbnails' or 'bookmarks'\r\n  const [viewMode, setViewMode] = useState('single'); // 'single', 'spread', 'presentation'\r\n  const [isFullscreen, setIsFullscreen] = useState(false);\r\n  const [bookmarks, setBookmarks] = useState([]);\r\n  const [showSearch, setShowSearch] = useState(false);\r\n  const [highlightMode, setHighlightMode] = useState(false);\r\n  const [selectedColor, setSelectedColor] = useState('#FFD700');\r\n  const containerRef = useRef(null);\r\n  const pageRefsMap = useRef({});\r\n\r\n  const handleDocumentLoad = ({ numPages: nextNumPages }) => {\r\n    setNumPages(nextNumPages || 1);\r\n    setIsLoading(false);\r\n    setPageNumber(1);\r\n  };\r\n\r\n  const zoomIn = useCallback(() => {\r\n    setScale(s => Math.min(2.5, s + 0.1));\r\n  }, []);\r\n\r\n  const zoomOut = useCallback(() => {\r\n    setScale(s => Math.max(0.5, s - 0.1));\r\n  }, []);\r\n\r\n  const fitToWidth = useCallback(() => {\r\n    setScale(1.0);\r\n  }, []);\r\n\r\n  const toggleFullscreen = useCallback(() => {\r\n    setIsFullscreen(v => !v);\r\n  }, []);\r\n\r\n  const toggleSidebar = useCallback(() => {\r\n    setShowSidebar(v => !v);\r\n  }, []);\r\n\r\n  const toggleBookmark = useCallback(() => {\r\n    setBookmarks(prev => {\r\n      const exists = prev.includes(pageNumber);\r\n      if (exists) {\r\n        return prev.filter(p => p !== pageNumber);\r\n      } else {\r\n        return [...prev, pageNumber].sort((a, b) => a - b);\r\n      }\r\n    });\r\n  }, [pageNumber]);\r\n\r\n  const goToPage = useCallback((page) => {\r\n    setPageNumber(Math.max(1, Math.min(numPages || 1, page)));\r\n  }, [numPages]);\r\n\r\n  const goNext = useCallback(() => {\r\n    goToPage(pageNumber + (viewMode === 'spread' ? 2 : 1));\r\n  }, [pageNumber, viewMode, goToPage]);\r\n\r\n  const goPrev = useCallback(() => {\r\n    goToPage(pageNumber - (viewMode === 'spread' ? 2 : 1));\r\n  }, [pageNumber, viewMode, goToPage]);\r\n\r\n  const rotate = useCallback(() => {\r\n    setRotation(r => (r + 90) % 360);\r\n  }, []);\r\n\r\n  // Keyboard shortcuts\r\n  useEffect(() => {\r\n    const handleKeyDown = (e) => {\r\n      if (e.key === 'Escape') {\r\n        if (showSearch) {\r\n          setShowSearch(false);\r\n        } else if (highlightMode) {\r\n          setHighlightMode(false);\r\n        } else if (isFullscreen) {\r\n          setIsFullscreen(false);\r\n        } else {\r\n          onClose();\r\n        }\r\n      }\r\n      if (e.key === 'ArrowLeft') goPrev();\r\n      if (e.key === 'ArrowRight') goNext();\r\n      if ((e.ctrlKey || e.metaKey) && e.key === '+') {\r\n        e.preventDefault();\r\n        zoomIn();\r\n      }\r\n      if ((e.ctrlKey || e.metaKey) && e.key === '-') {\r\n        e.preventDefault();\r\n        zoomOut();\r\n      }\r\n      if ((e.ctrlKey || e.metaKey) && e.key === '0') {\r\n        e.preventDefault();\r\n        fitToWidth();\r\n      }\r\n      if (e.key === 'f' || e.key === 'F') {\r\n        if (e.ctrlKey || e.metaKey) {\r\n          e.preventDefault();\r\n          setShowSearch(true);\r\n        } else {\r\n          e.preventDefault();\r\n          toggleFullscreen();\r\n        }\r\n      }\r\n      if (e.key === 'b' || e.key === 'B') {\r\n        e.preventDefault();\r\n        toggleBookmark();\r\n      }\r\n      if (e.key === 'h' || e.key === 'H') {\r\n        if (e.ctrlKey || e.metaKey) {\r\n          e.preventDefault();\r\n          setHighlightMode(v => !v);\r\n        }\r\n      }\r\n    };\r\n\r\n    window.addEventListener('keydown', handleKeyDown);\r\n    return () => window.removeEventListener('keydown', handleKeyDown);\r\n  }, [pageNumber, numPages, isFullscreen, viewMode, showSearch, highlightMode, goPrev, goNext, zoomIn, zoomOut, fitToWidth, toggleFullscreen, toggleBookmark, onClose]);\r\n\r\n  // Render pages based on view mode\r\n  const pagesToRender = useMemo(() => {\r\n    if (viewMode === 'spread' && pageNumber % 2 !== 0) {\r\n      return [pageNumber, pageNumber + 1].filter(p => p <= numPages);\r\n    } else if (viewMode === 'spread' && pageNumber % 2 === 0) {\r\n      return [pageNumber, pageNumber + 1].filter(p => p <= numPages);\r\n    }\r\n    return [pageNumber];\r\n  }, [pageNumber, viewMode, numPages]);\r\n\r\n  return (\r\n    <div\r\n      className={`elr-overlay ${isFullscreen ? 'fullscreen' : ''}`}\r\n      onClick={onClose}\r\n    >\r\n      <div\r\n        className={`elr-container ${isFullscreen ? 'fullscreen' : ''}`}\r\n        onClick={(e) => e.stopPropagation()}\r\n      >\r\n        {/* Header Toolbar */}\r\n        <div className=\"elr-header\">\r\n          <div className=\"elr-header-left\">\r\n            <button\r\n              className=\"elr-icon-btn\"\r\n              onClick={toggleSidebar}\r\n              title=\"Toggle sidebar (Ctrl+Shift+F)\"\r\n            >\r\n              <FiMenu size={20} />\r\n            </button>\r\n            <div className=\"elr-title-section\">\r\n              <div className=\"elr-title\">{title}</div>\r\n              {author && <div className=\"elr-author\">by {author}</div>}\r\n            </div>\r\n          </div>\r\n\r\n          <div className=\"elr-header-center\">\r\n            <button\r\n              className=\"elr-icon-btn\"\r\n              onClick={goPrev}\r\n              disabled={pageNumber <= 1}\r\n              title=\"Previous page (←)\"\r\n            >\r\n              <FiChevronLeft size={20} />\r\n            </button>\r\n            <input\r\n              type=\"number\"\r\n              min=\"1\"\r\n              max={numPages}\r\n              value={pageNumber}\r\n              onChange={(e) => goToPage(parseInt(e.target.value) || 1)}\r\n              className=\"elr-page-input\"\r\n              title=\"Go to page\"\r\n            />\r\n            <span className=\"elr-page-separator\">/</span>\r\n            <span className=\"elr-page-total\">{numPages || 0}</span>\r\n            <button\r\n              className=\"elr-icon-btn\"\r\n              onClick={goNext}\r\n              disabled={pageNumber >= (numPages || 1)}\r\n              title=\"Next page (→)\"\r\n            >\r\n              <FiChevronRight size={20} />\r\n            </button>\r\n          </div>\r\n\r\n          <div className=\"elr-header-right\">\r\n            {/* Search Box */}\r\n            {showSearch && (\r\n              <div className=\"elr-search-box\">\r\n                <FiSearch size={18} />\r\n                <input\r\n                  type=\"text\"\r\n                  placeholder=\"Search in document...\"\r\n                  autoFocus\r\n                  className=\"elr-search-input\"\r\n                />\r\n                <button\r\n                  className=\"elr-icon-btn-small\"\r\n                  onClick={() => setShowSearch(false)}\r\n                  title=\"Close search\"\r\n                >\r\n                  <FiX size={16} />\r\n                </button>\r\n              </div>\r\n            )}\r\n\r\n            {!showSearch && (\r\n              <>\r\n                {/* View Mode Selector */}\r\n                <div className=\"elr-view-mode\">\r\n                  <button\r\n                    className={`elr-view-btn ${viewMode === 'single' ? 'active' : ''}`}\r\n                    onClick={() => setViewMode('single')}\r\n                    title=\"Single page view\"\r\n                  >\r\n                    <FiLayout size={18} />\r\n                  </button>\r\n                  <button\r\n                    className={`elr-view-btn ${viewMode === 'spread' ? 'active' : ''}`}\r\n                    onClick={() => setViewMode('spread')}\r\n                    title=\"Two-page spread view\"\r\n                  >\r\n                    <FiColumns size={18} />\r\n                  </button>\r\n                  <button\r\n                    className={`elr-view-btn ${viewMode === 'presentation' ? 'active' : ''}`}\r\n                    onClick={() => setViewMode('presentation')}\r\n                    title=\"Presentation mode\"\r\n                  >\r\n                    <FiPlay size={18} />\r\n                  </button>\r\n                </div>\r\n\r\n                {/* Zoom Controls */}\r\n                <div className=\"elr-zoom-controls\">\r\n                  <button\r\n                    className=\"elr-icon-btn\"\r\n                    onClick={zoomOut}\r\n                    title=\"Zoom out (Ctrl+-)\"\r\n                  >\r\n                    <FiZoomOut size={18} />\r\n                  </button>\r\n                  <span className=\"elr-zoom-level\">{Math.round(scale * 100)}%</span>\r\n                  <button\r\n                    className=\"elr-icon-btn\"\r\n                    onClick={zoomIn}\r\n                    title=\"Zoom in (Ctrl++)\"\r\n                  >\r\n                    <FiZoomIn size={18} />\r\n                  </button>\r\n                </div>\r\n\r\n                {/* Annotation Tools */}\r\n                <button\r\n                  className={`elr-icon-btn ${highlightMode ? 'active' : ''}`}\r\n                  onClick={() => setHighlightMode(v => !v)}\r\n                  title=\"Highlight text (Ctrl+H)\"\r\n                  style={{ color: highlightMode ? selectedColor : 'inherit' }}\r\n                >\r\n                  <FiEdit3 size={18} />\r\n                </button>\r\n\r\n                {/* More Actions */}\r\n                <button\r\n                  className=\"elr-icon-btn\"\r\n                  onClick={toggleBookmark}\r\n                  title=\"Toggle bookmark (B)\"\r\n                  style={{ color: bookmarks.includes(pageNumber) ? '#10b981' : 'inherit' }}\r\n                >\r\n                  <FiBookmark size={18} />\r\n                </button>\r\n\r\n                <button\r\n                  className=\"elr-icon-btn\"\r\n                  onClick={rotate}\r\n                  title=\"Rotate page\"\r\n                >\r\n                  <FiRotateCw size={18} />\r\n                </button>\r\n\r\n                {/* Search */}\r\n                <button\r\n                  className=\"elr-icon-btn\"\r\n                  onClick={() => setShowSearch(true)}\r\n                  title=\"Search (Ctrl+F)\"\r\n                >\r\n                  <FiSearch size={18} />\r\n                </button>\r\n\r\n                <button\r\n                  className=\"elr-icon-btn\"\r\n                  onClick={toggleFullscreen}\r\n                  title=\"Toggle fullscreen (F)\"\r\n                >\r\n                  {isFullscreen ? <FiMinimize2 size={18} /> : <FiMaximize2 size={18} />}\r\n                </button>\r\n\r\n                <button\r\n                  className=\"elr-icon-btn elr-close-btn\"\r\n                  onClick={onClose}\r\n                  title=\"Close reader (Esc)\"\r\n                >\r\n                  <FiX size={20} />\r\n                </button>\r\n              </>\r\n            )}\r\n          </div>\r\n        </div>\r\n\r\n        {/* Highlight Color Picker */}\r\n        {highlightMode && (\r\n          <div className=\"elr-color-picker-bar\">\r\n            <span className=\"elr-picker-label\">Highlight color:</span>\r\n            {['#FFD700', '#FF6B6B', '#4ECDC4', '#95E1D3', '#F38181'].map(color => (\r\n              <button\r\n                key={color}\r\n                className={`elr-color-btn ${selectedColor === color ? 'active' : ''}`}\r\n                style={{ backgroundColor: color }}\r\n                onClick={() => setSelectedColor(color)}\r\n                title={color}\r\n              />\r\n            ))}\r\n          </div>\r\n        )}\r\n\r\n        {/* Main Content Area */}\r\n        <div className=\"elr-content-wrapper\">\r\n          {/* Sidebar */}\r\n          {showSidebar && (\r\n            <aside className=\"elr-sidebar\">\r\n              {/* Sidebar Tabs */}\r\n              <div className=\"elr-sidebar-tabs\">\r\n                <button\r\n                  className={`elr-sidebar-tab ${sidebarTab === 'thumbnails' ? 'active' : ''}`}\r\n                  onClick={() => setSidebarTab('thumbnails')}\r\n                >\r\n                  Thumbnails\r\n                </button>\r\n                <button\r\n                  className={`elr-sidebar-tab ${sidebarTab === 'bookmarks' ? 'active' : ''}`}\r\n                  onClick={() => setSidebarTab('bookmarks')}\r\n                >\r\n                  Marks ({bookmarks.length})\r\n                </button>\r\n              </div>\r\n\r\n              {/* Thumbnails Tab */}\r\n              {sidebarTab === 'thumbnails' && (\r\n                <div className=\"elr-thumbnails\">\r\n                  {numPages && Array.from({ length: numPages }, (_, i) => i + 1).map(page => (\r\n                    <div\r\n                      key={page}\r\n                      className={`elr-thumbnail ${pageNumber === page ? 'active' : ''}`}\r\n                      onClick={() => goToPage(page)}\r\n                      title={`Page ${page}`}\r\n                    >\r\n                      <Document file={src} loading={null}>\r\n                        <Page pageNumber={page} scale={0.15} />\r\n                      </Document>\r\n                      <div className=\"elr-thumb-number\">{page}</div>\r\n                    </div>\r\n                  ))}\r\n                </div>\r\n              )}\r\n\r\n              {/* Bookmarks Tab */}\r\n              {sidebarTab === 'bookmarks' && (\r\n                <div className=\"elr-bookmarks\">\r\n                  {bookmarks.length === 0 ? (\r\n                    <div className=\"elr-empty-state\">\r\n                      <FiBookmark size={32} />\r\n                      <p>No bookmarks yet</p>\r\n                      <span className=\"elr-hint\">Click the bookmark icon to mark pages</span>\r\n                    </div>\r\n                  ) : (\r\n                    <div className=\"elr-bookmarks-list\">\r\n                      {bookmarks.map(page => (\r\n                        <button\r\n                          key={page}\r\n                          className={`elr-bookmark-item ${pageNumber === page ? 'active' : ''}`}\r\n                          onClick={() => goToPage(page)}\r\n                        >\r\n                          <FiBookmark size={16} />\r\n                          <span>Page {page}</span>\r\n                        </button>\r\n                      ))}\r\n                    </div>\r\n                  )}\r\n                </div>\r\n              )}\r\n            </aside>\r\n          )}\r\n\r\n          {/* PDF Viewer */}\r\n          <div className=\"elr-viewer\" ref={containerRef}>\r\n            {isLoading && (\r\n              <div className=\"elr-loading\">\r\n                <div className=\"elr-spinner\"></div>\r\n                <p>Loading PDF...</p>\r\n              </div>\r\n            )}\r\n\r\n            <Document\r\n              file={src}\r\n              onLoadSuccess={handleDocumentLoad}\r\n              loading={null}\r\n              error={\r\n                <div className=\"elr-error\">\r\n                  <p>Failed to load PDF. Please check the file